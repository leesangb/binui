{
  "name": "binui",
  "version": "1.0.0",
  "description": "sangbin's react component library",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && rollup -c",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --passWithNoTests",
    "release": "npm run build && npm version",
    "preversion": "npm run test && npm run build",
    "postversion": "git push --tags && npm publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "author": "Sangbin Lee <leesangbin@outlook.com>",
